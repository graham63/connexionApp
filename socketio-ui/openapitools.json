{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "vue3-api-client": {
        "generatorName": "javascript",
        "inputSpec": "http://localhost:8080/openapi.yaml",
        "output": "./src/lib",
        "additionalProperties": {
          "usePromises": true,
          "useES6": true
        }
      }
    }
  }
}

